using System.Collections;
using System;
using System.Collections.Generic;
using UnityEngine;

public class Calories : MonoBehavior {
    public int number;
    public string name;
    //
    public double energy;
    public double protein;
    public double lipid;
    public double carb;

    public Calories(int number, string name, double energy, double protein, double lipid, double carb){
        this.number = number;
        this.name = name;

        this.energy = energy;
        this.protein = protein;
        this.lipid = lipid;
        this.carb = carb;
    }
}